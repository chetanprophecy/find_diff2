type src_env_t =
utf8 record
  // Natural Key
  cli_src_env_id_t                 src_env_id                 /* VARCHAR(4)  NOT NULL */;
  
  // Surrogate Key
  cli_integer_id_t                 cli_src_env_id;
  
  // Data
  string(int, maximum_length=50)   src_env_nm                 /* VARCHAR(50) NOT NULL */;
  string(int, maximum_length=60)   legacy_bus_nm              /* VARCHAR(60) NOT NULL */;
  
  cli_datetime_t                   ids_create_dttm    = NULL  /* TIMESTAMP(6)         */;
  cli_datetime_t                   ids_updt_dttm      = NULL  /* TIMESTAMP(6)         */;
  ids_rec_stat_cd_t                ids_rec_stat_cd    = NULL;
end;

type carrier_t =
utf8 record
  // Natural Key
  cli_carrier_id_t                 carrier_id                 /* VARCHAR(30) NOT NULL */;
  
  // Surrogate Key
