type prescriber_t =
utf8 record
  cli_integer_id_t                 cli_prescriber_id;  
  cli_string_t                     phy_pbr_key;
  
  //
  // Note that there are a lot more prescriber data fields available in the CPP Inbound
  // Extract from IDS that we can add to this as required. Note that some additional data
  // fields may be required for Case Management or Fulfullment. Please see the following
  // file to see what is available:
  //
  //     $PUB_CLINICAL_DATAHUB_DML/inbound/ids/d_prescriber_cli_in_t.dml
  //

// PT: I feel that these should not be preserved here. Instead, we should use DEA ID
// and NPI ID from the Prescriber Alias lookup. After all, a prescriber can have multiple
// DEA ID and NPI ID.

  cli_dea_id_t                     dea_id             = NULL  /* VARCHAR(15)          */;
  cli_npi_id_t                     npi_id             = NULL  /* VARCHAR(10)          */;
  
  cli_first_nm_t                   first_nm           = NULL  /* VARCHAR(25)          */;
  cli_mid_init_nm_t                mid_init_nm        = NULL  /* CHAR(1)              */;
  cli_last_nm_t                    last_nm            = NULL  /* VARCHAR(35)          */;
  
  cli_addr_t                       pbr_addr1          = NULL  /* VARCHAR(55)          */;
  cli_addr_t                       pbr_addr2          = NULL  /* VARCHAR(55)          */;

  cli_city_nm_t                    city_nm            = NULL  /* VARCHAR(30)          */;
  cli_st_cd_t                      st_cd              = NULL  /* VARCHAR(3)           */;
  string(int, maximum_length=15)   zip_cd             = NULL  /* VARCHAR(15)          */;
  
  cli_src_env_id_t                 src_env_id         = NULL  /* VARCHAR(4)           */;
  cli_datetime_t                   ids_create_dttm    = NULL  /* TIMESTAMP(6)         */;
  cli_datetime_t                   ids_updt_dttm      = NULL  /* TIMESTAMP(6)         */;
  ids_rec_stat_cd_t                ids_rec_stat_cd    = NULL;
end;

