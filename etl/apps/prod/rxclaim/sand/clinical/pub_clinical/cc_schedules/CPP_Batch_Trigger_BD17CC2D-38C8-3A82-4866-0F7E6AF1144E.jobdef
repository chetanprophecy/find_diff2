    </timeConstraint>
    <fileAndEventConstraints>
      <fileConstraint fileRetryIntervalSec="120" fileURL="$AI_SERIAL_TEMP/cpp_cc_batch_trigger.ctl" isFirstMatchOnly1X50="false" isRegExSyntax1X40="false" isRepeatableForFileCreationTime="false"/>
      <fileConstraint fileRetryIntervalSec="120" fileURL="$PUB_CLINICAL_DATAHUB_SERIAL_CONTROL/ids/ids_rxclaim_reconciliation_trigger.ctl" isFirstMatchOnly1X50="false" isRegExSyntax1X40="false" isRepeatableForFileCreationTime="false"/>
    </fileAndEventConstraints>
    <predecessors>
      <predecessor jobDefinitionGuid="408673A8-864B-E096-6708-E0B1222DAE22"/>
    </predecessors>
  </constraintSet>

</jobDefinition>
