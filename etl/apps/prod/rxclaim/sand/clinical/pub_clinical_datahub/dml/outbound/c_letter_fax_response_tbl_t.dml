record
  utf8 string("\x01", maximum_length=40) letter_fax_response_id /*VARCHAR2(40) NOT NULL*/;
  utf8 string("\x01", maximum_length=20) case_fulfillment_id /*VARCHAR2(20) NOT NULL*/;
  utf8 string("\x01", maximum_length=20) fax_num = NULL("") /*VARCHAR2(20)*/;
  datetime("YYYYMMDD HH24:MI:SS")("\x01") fax_request_dt /*DATE NOT NULL*/;
  datetime("YYYYMMDD HH24:MI:SS")("\x01") fax_request_pickup_dt /*DATE NOT NULL*/;
  datetime("YYYYMMDD HH24:MI:SS")("\x01") fax_request_process_dt /*DATE NOT NULL*/;
  decimal("\x01",0, maximum_length=39) num_of_attempts = NULL("") /*NUMBER(38)*/;
  utf8 string("\x01", maximum_length=10) fax_stts_type_id /*VARCHAR2(10) NOT NULL*/;
  utf8 string("\x01", maximum_length=255) fax_stts_id /*VARCHAR2(10) NOT NULL*/;
  utf8 string("\x01", maximum_length=10) no_of_pages_transmitted = NULL("") /*VARCHAR2(10)*/;
  datetime("YYYYMMDD HH24:MI:SS")("\x01") fax_response_received_dt = NULL("") /*DATE*/;
  datetime("YYYYMMDD HH24:MI:SS")("\x01") update_timestamp = NULL("") /*DATE*/;
  utf8 string("\x01", maximum_length=20) updated_by = NULL("") /*VARCHAR2(20)*/;
  datetime("YYYYMMDD HH24:MI:SS")("\x01") create_timestamp /*DATE NOT NULL*/;
  utf8 string("\x01", maximum_length=20) created_by /*VARCHAR2(20) NOT NULL*/;
  utf8 string("\x01", maximum_length=1) actn_flag = NULL('');
  string(1) newline = '\n';
end

