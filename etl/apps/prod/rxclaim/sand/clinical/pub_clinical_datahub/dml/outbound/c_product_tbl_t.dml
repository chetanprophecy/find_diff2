record
  utf8 string("\x01", maximum_length=15) product_id /*VARCHAR2(15) NOT NULL*/;
  utf8 string("\x01", maximum_length=50) product_name /*VARCHAR2(50) NOT NULL*/;
  utf8 string("\x01", maximum_length=10) product_type_cd /*VARCHAR2(10) NOT NULL*/;
  utf8 string("\x01", maximum_length=250) product_descr = NULL("") /*VARCHAR2(250)*/;
  datetime("YYYYMMDD HH24:MI:SS")("\x01") update_timestamp = NULL("") /*DATE*/;
  utf8 string("\x01", maximum_length=20) updated_by = NULL("") /*VARCHAR2(20)*/;
  datetime("YYYYMMDD HH24:MI:SS")("\x01") create_timestamp /*DATE NOT NULL*/;
  utf8 string("\x01", maximum_length=20) created_by /*VARCHAR2(20) NOT NULL*/;
  utf8 string("\x01", maximum_length=50) product_sub_type_name = NULL("") /*VARCHAR2(50)*/;
  utf8 string("\x01", maximum_length=1) stts = NULL("") /*CHAR(1)*/;
  utf8 string("\x01", maximum_length=1) actn_flag = NULL('');
  string(1) newline = "\n";
end

