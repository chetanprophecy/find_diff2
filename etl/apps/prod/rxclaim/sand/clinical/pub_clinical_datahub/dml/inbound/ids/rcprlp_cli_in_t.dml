type rcprlp_cli_in_t =
record
        string("\x01", maximum_length=12) phr_id /*VARCHAR(12)*/;
        decimal("\x01",0)   phr_relshp_seq_no /*INTEGER*/;
        string("\x01", maximum_length=6) phr_affl_cd = NULL("") /*VARCHAR(6)*/;
        string("\x01", maximum_length=12) phr_pmt_cntr_id = NULL("") /*VARCHAR(12)*/;
        string("\x01", maximum_length=1) phr_relshp_stat_cd = NULL("") /*CHAR(1)*/;
