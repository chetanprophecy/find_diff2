type rcpmdp_cli_in_t =

record
  decimal("\x01",maximum_length=5) src_sys_id = 0;
  decimal("\x01",maximum_length=9) prod_key /*BIGINT*/;
  string("\x01",charset="iso-8859-15", maximum_length=6) mod_cd /*VARCHAR(6) NOT NULL*/;
  string("\x01",charset="iso-8859-15", maximum_length=1) pmd_stat_cd /*CHAR(1) NOT NULL*/;
  string("\x01",charset="iso-8859-15", maximum_length=1) pmd_orig_cd /*CHAR(1) NOT NULL*/;
  datetime("YYYYMMDDHH24MISS")("\x01")   ids_create_dttm = NULL("");
  string("\x01", maximum_length=20)   ids_create_uid = NULL("");
  decimal("\x01",maximum_length=20)   ids_create_run_id =   0;
  datetime("YYYYMMDDHH24MISS")("\x01")   ids_updt_dttm = NULL("");
  string("\x01", maximum_length=20)   ids_updt_uid = NULL("");
  decimal("\x01",maximum_length=20)   ids_updt_run_id =   0;
  string("\x01",maximum_length=2)   ids_rec_stat_cd = NULL("");
  string(1)  newline =  "\n";
end ;

type rcpmdp_cli_in_vec_t = rcpmdp_cli_in_t[int];

metadata type = rcpmdp_cli_in_t;
