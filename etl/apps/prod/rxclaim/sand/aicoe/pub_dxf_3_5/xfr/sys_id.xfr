decimal("")
out :: name_to_sys_id_local(string("") sys_name, string("") ai_rel_loc) inline =
begin
  out ::
    if (sys_name == "meta")
      // For local "systems", use the name of the target.
      name_to_sys_id_local(ai_rel_loc, sys_name)
    else
      name_to_sys_id(sys_name);
end;

decimal("")
out :: name_to_sys_id(string("") sys_name) inline =
begin
  out :: name_pbs_to_sys_id(sys_name, 1);
end;

