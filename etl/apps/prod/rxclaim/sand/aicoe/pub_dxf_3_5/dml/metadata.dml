type ecg_metadata = record
  string("|") source_name = NULL("") ;
  string("|") schema_name = NULL("");
  string("|") table_name = NULL("");
  string("|") column_name = NULL("") ;
  string("|") data_type = NULL("");
  string("|") data_length = NULL("");
  string("|") data_scale = NULL("") ;
  string("|") format = NULL("");
  string("|") primary_keys = NULL("");
  string("|") column_id = NULL("");
  string("\n") column_alias_name = NULL("");
end;

metadata type = ecg_metadata;
