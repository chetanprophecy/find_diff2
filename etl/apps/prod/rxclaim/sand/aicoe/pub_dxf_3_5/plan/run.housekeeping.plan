{30001002|XXparameter|ab_task_script|#!/bin/ksh -e


LOGFILE=$LOG_FILE
echo -e "Please refer LOG - '$LOGFILE' for details.\\n" \| tee -a $LOGFILE
rm -f $ARCHIVE_FILE_LIST
rm -f $CONS_ARCHIVE_FILE_LST
touch $ARCHIVE_FILE_LIST
touch $CONS_ARCHIVE_FILE_LST
export FLAG=0

for LINE in `echo $HK_PATTERN` 
do
        FLAG=0
        DIR_NM=`echo $LINE \| awk -F"\|" '\{print $1\}'`
        ARC_DIR_NM=`echo $LINE \| awk -F"\|" '\{print $2\}'`
        PATTERN=`echo $LINE \| awk -F"\|" '\{print $3\}'`
        RETENTION=`echo $LINE \| awk -F"\|" '\{print $4\}'`
        DIR_TYPE=`m_eval "multifile_information('$DIR_NM').file_type"`
        DIR_TYPE_TRIMMED=`eval echo $DIR_TYPE\|tr -d " "`
        echo "DIR TYPE              :: $DIR_TYPE_TRIMMED" \| tee -a $LOGFILE
        echo "DIRECTORY NAME    :: $DIR_NM" \| tee -a $LOGFILE
        echo "ARCHIVE DIRECTORY :: $ARC_DIR_NM" \| tee -a $LOGFILE
        echo "PATTERN :: $PATTERN" \| tee -a $LOGFILE
        echo "DAYS OF RETENTION :: $RETENTION" \| tee -a $LOGFILE

        if [[ ! -d $DIR_NM ]]
        then
                echo -e "\\n*******************************************************************************************************************\\n"   \| tee -a $LOGFILE
                echo "Directory does not exist!!" \| tee -a $LOGFILE
                echo -e "\\n*******************************************************************************************************************\\n"   \| tee -a $LOGFILE
                exit 1
        fi

        if [[ ! -d $ARC_DIR_NM && $DIR_TYPE_TRIMMED -eq "MDIR"  ]]
        then
                echo -e "\\n*******************************************************************************************************************\\n"   \| tee -a $LOGFILE
                echo "Directory does not exist!! Creating MFS Archive Directory \\n" \| tee -a $LOGFILE
                m_mkdir -p $ARC_DIR_NM
                echo "MFS Archive Directory Created successfully" \| tee -a $LOGFILE
                echo -e "\\n*******************************************************************************************************************\\n"   \| tee -a $LOGFILE

         fi


        if [[! -d $ARC_DIR_NM && $DIR_TYPE_TRIMMED -eq "DIR"  ]] ;then
                echo "Directory does not exist!! Creating Serial Archive Directory \\n" \| tee -a $LOGFILE
                mkdir -p $ARC_DIR_NM
                echo "Serial Archive Directory Created Succefully" \| tee -a $LOGFILE
                echo -e "\\n*******************************************************************************************************************\\n"   \| tee -a $LOGFILE
         fi

       

        echo -e "\\n*******************************************************************************************************************\\n"   \| tee -a $LOGFILE

        echo -e "Finding files in '$DIR_NM' directory:\\n" \| tee -a $LOGFILE
        echo -e "Archive File List :   " $ARCHIVE_FILE_LIST  \| tee -a $LOGFILE
        echo " find $DIR_NM -mtime +$RETENTION -type f -name '$PATTERN' > $ARCHIVE_FILE_LIST " \| tee -a $LOGFILE
        

        list=`find $DIR_NM -mtime +$RETENTION -type f -iname "$PATTERN" ! -iname ".*" \| egrep -v "$ARC_DIR_NM*"\|\|true`  
        echo -e "$list"\|sed "/^$/d" > $ARCHIVE_FILE_LIST

        export COUNT=`eval wc -l $ARCHIVE_FILE_LIST \| cut -d " " -f1 `
        echo "COUNNT   :"  $COUNT \| tee -a $LOGFILE
        if [[ $DRY_RUN -eq 0 && $COUNT -ne 0  && $RETENTION -ne 0  ]] ;then
                        
                                if [[ $DIR_TYPE_TRIMMED -eq "MDIR" ]] ;then
                                        echo "Moving Files from '$DIR_NM' to '$ARC_DIR_NM'" \| tee -a $LOGFILE
                                        eval cat $ARCHIVE_FILE_LIST \| xargs -i -P 5 m_mv \{\} $ARC_DIR_NM 
                                        rc=$?
                                elif [[ $DIR_TYPE_TRIMMED -eq "DIR" ]] ; then
                                        echo "Moving Files from '$DIR_NM' to '$AI_SERIAL_ARCHIVE'" \| tee -a $LOGFILE
                                        eval cat $ARCHIVE_FILE_LIST \| xargs -i -P 5 mv \{\} $ARC_DIR_NM 
                                        rc=$?
                                fi

                                if [[ $rc -eq 0 ]]; then
                                        echo "Archival of Files from '$DIR_NM' to '$ARC_DIR_NM' completed successfully" \| tee -a $LOGFILE
                                        FLAG=1
                                else
                                        echo -e "Archival Files from '$DIR_NM' to '$ARC_DIR_NM' failed with error code :: $?. Now exiting..\\n" \| tee -a $LOGFILE
                                        exit $rc
                                fi
 
            else 
                
                echo "Either DRY_RUN is set to 1 or Count of Archive File is 0 "  \| tee -a $LOGFILE
        fi

        if [[ $FLAG -eq 0 ]]; then
	  echo -e "No files were Archived from '$DIR_NM' !!\\n" \| tee -a $LOGFILE
        fi

        eval cat $ARCHIVE_FILE_LIST >> $CONS_ARCHIVE_FILE_LST
        
done




echo -e "*******************************************************************************************************************\\n"   \| tee -a $LOGFILE|3|1|l|@{0|}}
