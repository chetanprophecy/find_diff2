{2031500001|XXGtask|10|0|19|0|{@{30100001|XXparameter_set|@@@@{{30001002|XXparameter|!prototype_path|$AB_HOME/lib/components/dtm/tasks/Program_Task.task|3|9|Pw|@{0|}}
{30001002|XXparameter|ab_task_script|#!/bin/ksh -e


#########################################################################################################
#                                                                                                       #
#This script purges serial and MFS files                                         #
#
#########################################################################################################


LOGFILE=$LOG_FILE
rm -f $PURGE_FILE_LIST
rm -f $CONS_PURGE_FILE_LST
touch $PURGE_FILE_LIST
touch $CONS_PURGE_FILE_LST
export FLAG=0

echo -e "\\n*******************************************************************************************************************\\n"   \| tee -a $LOGFILE
echo -e "\\n************ PURGE PROCESS STARTED *****************\\n"   \| tee -a $LOGFILE


for LINE in `echo $HK_PATTERN`
do
        FLAG=0
        DIR_NM=`echo $LINE \| awk -F"\|" '\{print $1\}'` 
        ARC_DIR_NM=`echo $LINE \| awk -F"\|" '\{print $2\}'`
        PATTERN=`echo $LINE \| awk -F"\|" '\{print $3\}'`
        RETENTION=`echo $LINE \| awk -F"\|" '\{print $5\}'`
        DIR_TYPE=`m_eval "multifile_information('$ARC_DIR_NM').file_type"`
        DIR_TYPE_TRIMMED=`eval echo $DIR_TYPE\|tr -d " "`
        echo "DIR TYPE              :: $DIR_TYPE_TRIMMED" \| tee -a $LOGFILE
        echo "ARCHIVE DIRECTORY :: $ARC_DIR_NM" \| tee -a $LOGFILE
        echo "PATTERN :: $PATTERN" \| tee -a $LOGFILE
        echo "DAYS OF RETENTION :: $RETENTION" \| tee -a $LOGFILE

        
        echo -e "\\n*******************************************************************************************************************\\n"   \| tee -a $LOGFILE

        echo -e "Finding files in '$ARC_DIR_NM' directory:\\n" \| tee -a $LOGFILE
        echo -e "Purge File List :   " $PURGE_FILE_LIST  \| tee -a $LOGFILE
        echo "find $ARC_DIR_NM  -type f -name '$PATTERN' -mtime +$RETENTION > $PURGE_FILE_LIST " \| tee -a $LOGFILE
        eval find $ARC_DIR_NM -mtime +$RETENTION -type f '-iname "$PATTERN" ! -iname ".*"' > $PURGE_FILE_LIST 
        eval find $DIR_NM -mtime +$RETENTION -type f '-iname "$PATTERN" ! -iname ".*"' >> $PURGE_FILE_LIST 
        if [[ $DRY_RUN -eq 0 && `wc -l $PURGE_FILE_LIST \| cut -d " " -f1 ` -ne 0 && $RETENTION -ne 0 ]] ;then
                        
                                if [[ $DIR_TYPE_TRIMMED == "MDIR" ]] ;then
                                        echo "Deleting Files from '$ARC_DIR_NM'" \| tee -a $LOGFILE
                                        eval cat $PURGE_FILE_LIST \| xargs -i -P 5 m_rm -f \{\}
                                        rc=$?
                                elif [[ $DIR_TYPE_TRIMMED == "DIR" ]] ; then
                                        echo "Deleting Files from $ARC_DIR_NM'" \| tee -a $LOGFILE
                                        eval cat $PURGE_FILE_LIST \| xargs -i -P 5 rm -f \{\}
                                        rc=$?
                                fi

                                if [[ $rc == 0 ]]; then
                                        echo "Purging from '$ARC_DIR_NM' completed successfully" \| tee -a $LOGFILE
                                        FLAG=1
                                else
                                        echo -e "Purging from '$ARC_DIR_NM' failed with error code :: $?. Now exiting..\\n" \| tee -a $LOGFILE
                                        exit $rc
                                fi
     
            fi

      eval cat $PURGE_FILE_LIST >> $CONS_PURGE_FILE_LST

      if [[ $FLAG -eq 0 ]]; then
      	echo -e "No files were purged in '$ARC_DIR_NM' directory !!\\n" \| tee -a $LOGFILE
      fi 

done



echo -e "*******************************************************************************************************************\\n"   \| tee -a $LOGFILE|3|1|l|@{0|}}
}}@Run Purge Process|@@{0|}308500|151500|0|15000|15000|0|0|0.0|0.0|{}{0|}0|}}
