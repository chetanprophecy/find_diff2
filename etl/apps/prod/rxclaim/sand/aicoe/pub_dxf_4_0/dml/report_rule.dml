type rule_target_t =
record
  decimal("",0) constraint_num;
  utf8 string("") constraint_expr = "";
  utf8 string("") rule_expr = "";
  utf8 string("") target_markup = "";
end;

type report_rules_t =
record
  utf8 string("") source_markup;
  decimal("",0) constraint_num;
  utf8 string("") constraint;
  utf8 string("") rule;
  utf8 string("") target_markup;
  utf8 string("") description = "";
end;

metadata type = report_rules_t;
