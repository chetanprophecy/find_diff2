  string("\x01",charset="iso-8859-15", maximum_length=30) latest_status = NULL("");
  date("YYYYMMDD")("\x01") latest_status_date = NULL("");
  string("\x01",charset="iso-8859-15", maximum_length=10) prvdr_npi_id = NULL("");
  string("\x01",charset="iso-8859-15", maximum_length=100) drg_nm = NULL("");
