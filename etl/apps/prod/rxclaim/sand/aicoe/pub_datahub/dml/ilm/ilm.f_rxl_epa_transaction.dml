type logical_pcms_f_rxl_epa_transaction_t =
record
  date("YYYYMMDD")("\x01") txn_create_dt = NULL("");
  decimal("\x01",0) task_id = NULL("");
  string("\x01",charset="iso-8859-15", maximum_length=50) pa_case_id = NULL("");
  date("YYYYMMDD")("\x01") pa_case_id_create_dt = NULL("");
  string("\x01",charset="iso-8859-15", maximum_length=30) txn_status = NULL("");
