out :: rollup(in) =
begin
  out.claim_cntr  ::  	sum(in.claim_cntr);
out.days_sply  ::  	sum(in.days_sply);
out.drg_qty  ::  	sum(in.drg_qty);
out.app_ingred_cost_paid  ::  	sum(in.app_ingred_cost_paid);
out.app_dispensing_fee  ::  	sum(in.app_dispensing_fee);
out.app_sales_tax_paid  ::  	sum(in.app_sales_tax_paid);
out.app_copay_amt  ::  	sum(in.app_copay_amt);
out.app_due_amt  ::  	sum(in.app_due_amt);
out.sbm_usual_customary_amt  ::  	sum(in.sbm_usual_customary_amt);
out.extd_awp_cost  ::  	sum(in.extd_awp_cost);
out.clt_ingred_cost_paid  ::  	sum(in.clt_ingred_cost_paid);
out.clt_dispensing_fee  ::  	sum(in.clt_dispensing_fee);
out.clt_sales_tax_paid  ::  	sum(in.clt_sales_tax_paid);
out.clt_patient_pay_amt  ::  	sum(in.clt_patient_pay_amt);
out.clt_due_amt  ::  	sum(in.clt_due_amt);

  out.* :: in.*;
end;
