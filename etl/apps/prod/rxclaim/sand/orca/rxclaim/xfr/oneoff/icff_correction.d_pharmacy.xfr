/*Reformat operation*/
out::reformat(in)=
begin
out.src_env_sk:: if (in.dxf_src_sys_id != in.src_env_sk && in.dxf_src_sys_id>0) in.dxf_src_sys_id else in.src_env_sk;
out.dxf_src_sys_id:: if (in.dxf_src_sys_id != in.src_env_sk && in.src_env_sk>0) in.src_env_sk else in.dxf_src_sys_id;
//out.src_env_sk :: if (in.src_env_sk == 0 or in.src_env_sk == -1) 660 else in.src_env_sk;
out.* :: in.*;
end;

