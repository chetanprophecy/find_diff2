//Added extra sign byte and decimal point byte to length for all DECIMAL fields.
record
  little endian integer(8) dxf_src_dataset_id = 0/*@ BizName:'DXF Source Dataset ID' BizComment:'The surrogate key for the DXF Process Log entry for this source.' @*/;
  little endian integer(4) dxf_src_rec_cnt = 0/*@ BizName:'DXF Source Record Count' BizComment:'Starts at 1 each time the Source File Name changes.' @*/;
  little endian integer(2) dxf_src_sys_id = 0/*@ BizName:'DXF Source System ID' BizComment:'The numeric identifier of this source.' @*/;
  string(unsigned integer(1)) dxf_src_file_name = ""/*@ BizName:'DXF Source File Name' BizComment:'The name of the source file, relative to $AI_SERIAL_PENDING.' @*/;
  string("\001", maximum_length=1) cdc_flag = NULL("");
  datetime("YYYY-MM-DD HH24:MI:SS.nnnnnn")("\001") cdc_ts = NULL("");
  decimal("\001", maximum_length=11) s5iin2 = NULL("");
  string("\001", maximum_length=6) s5d8c4 = NULL("");
  string("\001", maximum_length=1) s5qos3 = NULL("");
  string("\001", maximum_length=1) s5qps3 = NULL("");
  string("\001", maximum_length=10) s5akvn = NULL("");
  decimal("\001", maximum_length=9) s5c2dt = NULL("");
  decimal("\001", maximum_length=8) s5adtm = NULL("");
  string("\001", maximum_length=10) s5alvn = NULL("");
  string("\001", maximum_length=10) s5advn = NULL("");
  decimal("\001", maximum_length=9) s5bmdt = NULL("");
  decimal("\001", maximum_length=8) s5abtm = NULL("");
  string("\001", maximum_length=10) s5aevn = NULL("");
  string(1) newline = "\n";
end
