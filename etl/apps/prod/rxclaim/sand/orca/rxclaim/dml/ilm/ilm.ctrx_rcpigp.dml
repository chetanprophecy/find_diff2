  unsigned integer(8) dxf_sk = 0;
  decimal("\x01",maximum_length=9) prod_key;
  string("\x01",charset="iso-8859-15", maximum_length=1) pig_ing_type_cd;
  decimal("\x01",maximum_length=9) pig_ing_id;
  string("\x01",charset="iso-8859-15", maximum_length=40) pig_gnrc_nm;
