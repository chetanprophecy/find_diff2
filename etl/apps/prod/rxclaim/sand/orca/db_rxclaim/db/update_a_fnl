update CTR_100_A_FNL_MEMBER_MONTH FM
SET ACTIVE_DAYS_ELIG_MBR_CNT_NO = ME.CUST_MBR_ELIG_CNT,
	ACTIVE_DAYS_SUBSCRIBER_CNT_NO = ME.CUST_MBR_ELIG_CNT,
	UPDATE_TS = CURRENT_DATE,
	UPDATE_UID = 'From l_cust_mbr_elig'
FROM ORX_IDW_DM_PRD.ADMIN.L_CUSTOM_MEMBER_ELIGIBILITY ME
Where ME.CUST_MBR_ELIG_DT = to_date(PERD_START_DT_SK, 'YYYYMMDD') and FM.PERD_TYPE_SK=9 
and FM.PERD_START_DT_SK=20230701 and FM.UTILIZING_MBR_CNT_NO=0 
and FM.CAG_SK in (select distinct CAG_SK from ORX_IDW_DM_PRD..D_CAG where CARRIER_ID in ('CTR002X'))
and ME.CARRIER_ID in ('CTR002X') and ME.CUST_MBR_ELIG_DT=to_date('20230701', 'YYYYMMDD');
